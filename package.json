{
	"name": "melonpan-block-post-list",
	"version": "2.0.0",
	"description": "Melonpan Block Post List WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/melonpan-block-post-list/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
	"dependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/preset-typescript": "^7.12.7",
		"@babel/register": "^7.12.10",
		"@types/lodash": "^4.14.168",
		"@types/node": "^14.14.22",
		"@types/react-select": "^3.1.2",
		"@types/uuid": "^8.3.0",
		"@types/wordpress__blocks": "^6.4.12",
		"@types/wordpress__data": "^4.6.9",
		"@wordpress/blocks": "^6.25.2",
		"@wordpress/browserslist-config": "^3.0.0",
		"@wordpress/core-data": "^2.25.2",
		"@wordpress/element": "^2.19.0",
		"@wordpress/i18n": "^3.17.0",
		"array-move": "^3.0.1",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.5",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"mini-css-extract-plugin": "^1.3.4",
		"nib": "^1.1.2",
		"react": "^16.14.0",
		"react-dom": "^16.14.0",
		"react-select": "^4.0.2",
		"react-tiny-popover": "^6.0.4",
		"react-window": "^1.8.6",
		"replace-in-file": "^6.1.0",
		"stylus": "^0.54.8",
		"stylus-loader": "^4.3.3",
		"terser-webpack-plugin": "^5.1.1",
		"typescript": "^4.1.3",
		"uuid": "^8.3.2",
		"webpack": "^5.17.0",
		"webpack-cli": "^4.4.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.16.1",
		"@typescript-eslint/eslint-plugin": "^4.14.0",
		"@typescript-eslint/parser": "^4.14.0",
		"eslint": "^7.18.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"prettier": "^2.2.1"
	}
}
