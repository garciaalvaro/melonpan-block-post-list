{
	"name": "melonpan-block-post-list",
	"version": "1.0.0",
	"description": "Melonpan Block Post List WordPress Plugin",
	"homepage": "#",
	"scripts": {
		"dev": "webpack --mode development --watch --config ./.webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./.webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./.webpack/copy.config.babel.js && rm _extras/release/_temp.js",
		"version": "webpack --mode production --config ./.webpack/version.config.babel.js && rm _temp.js",
		"release": "rm -rf build _extras/release && npm run version && npm run prod && npm run copy",
		"type": "tsc"
	},
	"dependencies": {
		"array-move": "^2.1.0",
		"immer": "^3.2.0",
		"react-select": "^3.0.4",
		"react-tiny-popover": "^3.4.5",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/register": "^7.5.5",
		"@types/lodash": "^4.14.138",
		"@types/node": "^12.7.2",
		"@types/react-dom": "^16.9.0",
		"@types/react-select": "^3.0.4",
		"@types/uuid": "^3.4.5",
		"@types/wordpress__blocks": "^6.4.3",
		"@types/wordpress__data": "^4.6.2",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"css-loader": "^3.2.0",
		"file-loader": "^4.2.0",
		"mini-css-extract-plugin": "^0.8.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.6.2",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.7"
	}
}
